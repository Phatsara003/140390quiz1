<% include ../partials/header %>

<table class="table">
    <thead>
        <tr>
            <th>Code</th>
            <th>Title</th>
            <th>Credits</th>
            <th>Section</th>
        </tr>
    </thead>
    <tbody>
    <% subjects.forEach(function(subject) { %>
        <tr>
        <td scope="row"><%= subject.code %></td>

            <td><%= subject.code %></td>
            <td><%= subject.title %></td>
            <td><%= subject.credits %></td>
            <td><%= subject.section %></td>
        </tr>
        <% });%>
    </tbody>
</table>